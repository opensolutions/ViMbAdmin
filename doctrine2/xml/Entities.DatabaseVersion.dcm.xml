<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\DatabaseVersion" repository-class="Repositories\DatabaseVersion" schema="dbversion" table="dbversion">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="version" type="integer" nullable="false"/>
    <field name="name" type="string" length="255" nullable="false"/>
    <field name="applied_on" type="datetime" nullable="false"/>
  </entity>
</doctrine-mapping>
